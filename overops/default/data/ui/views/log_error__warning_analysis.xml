<form hideFilters="true">
  <label>Log Error &amp; Warning Analysis</label>
  <description>For VP of Eng, Dev Lead</description>
  <init></init>
  <fieldset submitButton="true" autoRun="true">
    <input type="dropdown" token="time">
      <label>Select a Time Range</label>
      <choice value="-4h">Last 4 Hours</choice>
      <choice value="-12h">Last 12 Hours</choice>
      <choice value="-24h">Last 24 Hours</choice>
      <choice value="-2d">Last 2 Days</choice>
      <choice value="-7d">Last 7 Days</choice>
      <choice value="-14d">Last 14 Days</choice>
      <choice value="-30d">Last 30 Days</choice>
      <default>-24h</default>
      <initialValue>-24h</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Top Log Errors</title>
      <chart>
        <search>
          <query>index=overops overops_events earliest=$time$ NOT labels=*Resolved* service=* event_type IN (log_error) |rename class as "Class" | stats sum(value) as Times distinct_count(eventid) as count by Class |sort -Class</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.abbreviation">none</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.abbreviation">none</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.mode">standard</option>
        <option name="charting.legend.placement">right</option>
        <option name="charting.lineWidth">2</option>
        <option name="refresh.display">progressbar</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </chart>
    </panel>
    <panel>
      <title>Top Log Warnings</title>
      <chart>
        <search>
          <query>index=overops overops_events earliest=$time$ NOT labels=*Resolved* service=* event_type IN (log_warning) |rename class as Class | stats sum(value) as Times distinct_count(eventid) as count by Class | sort -Class</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.abbreviation">none</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.abbreviation">none</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.abbreviation">none</option>
        <option name="charting.axisY2.enabled">0</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.showDataLabels">none</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.mode">standard</option>
        <option name="charting.legend.placement">right</option>
        <option name="charting.lineWidth">2</option>
        <option name="refresh.display">progressbar</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <search>
          <query>index=overops overops_events earliest=$time$ NOT labels=*Resolved* service=* event_type =log_error | stats sum(value) as Times distinct_count(eventid) as count by class, method  | strcat class "." method eventlocation | rename eventlocation as "Code Location" | rename count as "Error Count" | Table "Code Location", "Error Count", Times | sort -Times</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">true</option>
        <option name="wrap">true</option>
        <format type="number" field="Times">
          <option name="precision">0</option>
        </format>
        <format type="number" field="Error Count">
          <option name="precision">0</option>
        </format>
      </table>
    </panel>
    <panel>
      <table>
        <search>
          <query>index=overops overops_events NOT earliest=$time$ NOT labels=*Resolved* service=* event_type =log_warning | stats sum(value) as Times distinct_count(eventid) as count by class, method  | strcat class "." method eventlocation | rename eventlocation as "Code Location" | rename count as "Error Count" | Table "Code Location", "Error Count", Times | sort -Times</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">true</option>
        <option name="wrap">true</option>
        <format type="number" field="Times">
          <option name="precision">0</option>
        </format>
        <format type="number" field="Error Count">
          <option name="precision">0</option>
        </format>
      </table>
    </panel>
  </row>
</form>