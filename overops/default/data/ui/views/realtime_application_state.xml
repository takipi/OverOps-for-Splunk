      <title>Application View ( click any application to see more )</title>
      <table>
        <search>
          <query>index=overops metric=overops_events earliest=-24h latest=now service=$environment$ event_type!=caught_exception event_type=$event_type$ labels=$resurfaced$  NOT labels=*Resolved*
| rename value as today 
| stats sum(today) as Times sparkline(sum(today),1h) as "24h Trend" distinct_count(eventid) as "Unique Events" by application 
| join application [search index=overops metric=overops_events earliest=-48h latest=-24h service=$environment$ event_type!=caught_exception event_type=$event_type$ NOT labels=*Resolved*
| rename value as previous 
| stats sum(previous) as Previous  by application ] 
| eval percent_change=(Times-Previous)/Previous *100 
| rename application as Application 
| rename percent_change as "Δ% 24h" 
| table Application, "Unique Events", Times, "Δ% 24h", "24h Trend" 
| sort -Times
| appendpipe [| stats count as event_count|  where event_count = 0 ]</query>
