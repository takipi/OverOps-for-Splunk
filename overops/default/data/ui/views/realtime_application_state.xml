<form hideFilters="true">
  <label>Real-Time Application State</label>
  <description>For SRE, DevOps, IT Ops to evaluate application health.</description>
  <init>
    <set token="event_type">*</set>
    <set token="resurfaced">*</set>
  </init>
  <fieldset submitButton="true" autoRun="true">
    <input type="dropdown" token="time">
      <label>Select a Time Range</label>
      <choice value="-4h">Last 4 Hours</choice>
      <choice value="-12h">Last 12 Hours</choice>
      <choice value="-24h">Last 24 Hours</choice>
      <choice value="-2d">Last 2 Days</choice>
      <choice value="-7d">Last 7 Days</choice>
      <choice value="-14d">Last 14 Days</choice>
      <choice value="-30d">Last 30 Days</choice>
      <default>-24h</default>
      <initialValue>-24h</initialValue>
      <!-- set condition based on the label defined by <choice> -->
      <!-- Within each condition, specify a custom label for display -->
      <!-- Capture the selected value in the token, earliest_tok -->
      <change>
        <condition label="Last 4 Hours">
          <set token="span">30m</set>
          <set token="trend">4h</set>
          <set token="spark">-16h</set>
          <set token="range_label">Last 4 Hours</set>
        </condition>
        <condition label="Last 12 Hours">
          <set token="span">1h</set>
          <set token="trend">12h</set>
          <set token="spark">-120h</set>
          <set token="range_label">Last 4 Hours</set>
        </condition>
        <condition label="Last 24 Hours">
          <set token="span">1h</set>
          <set token="trend">24h</set>
          <set token="spark">-7d</set>
          <set token="range_label">Last 24 Hours</set>
        </condition>
        <condition label="Last 2 Days">
          <set token="span">2h</set>
          <set token="trend">48h</set>
          <set token="spark">-336h</set>
          <set token="range_label">Last 2 Days</set>
        </condition>
        <condition label="Last 7 Days">
          <set token="span">1d</set>
          <set token="trend">7d</set>
          <set token="spark">-4w</set>
          <set token="range_label">This Week</set>
        </condition>
        <condition label="Last 14 Days">
          <set token="span">1d</set>
          <set token="trend">14d</set>
          <set token="spark">-30d</set>
          <set token="range_label">Last 2 Weeks</set>
        </condition>
        <condition label="Last 30 Days">
          <set token="span">1w</set>
          <set token="trend">30d</set>
          <set token="spark">-90d</set>
          <set token="range_label">Last 30 Days</set>
        </condition>
      </change>
    </input>
    <input type="dropdown" token="environment">
      <label>Environment</label>
      <fieldForLabel>service</fieldForLabel>
      <fieldForValue>service</fieldForValue>
      <search>
        <query>index=overops metric=overops_events 
| dedup service 
| sort +service</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <choice value="*">All Environments</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <!-- set condition based on the label defined by <choice> -->
      <change>
        <condition label="All Environments">
          <set token="env_label"></set>
        </condition>
        <condition>
          <set token="env_label">Environment: $label$</set>
        </condition>
      </change>
    </input>
    <input type="dropdown" token="application">
      <label>Application</label>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>application</fieldForLabel>
      <fieldForValue>application</fieldForValue>
      <search>
        <query>index=overops metric=overops_events  service=$environment$ 
| dedup application 
| sort +application</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <choice value="*">All Applications</choice>
      <!-- set condition based on the label defined by <choice> -->
      <change>
        <condition label="All Applications">
          <set token="app_label"></set>
        </condition>
        <condition>
          <set token="app_label">Application: $label$</set>
        </condition>
      </change>
    </input>
    <input type="dropdown" token="server">
      <label>Server</label>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>server</fieldForLabel>
      <fieldForValue>server</fieldForValue>
      <search>
        <query>index=overops metric=overops_events  service=$environment$ application= $application$ 
| dedup server 
| sort +server</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <choice value="*">All Servers</choice>
      <!-- set condition based on the label defined by <choice> -->
      <change>
        <condition label="All Servers">
          <set token="serv_label"></set>
        </condition>
        <condition>
          <set token="serv_label">Server: $label$</set>
        </condition>
      </change>
    </input>
    <input type="dropdown" token="event_type">
      <label>Event Type</label>
      <fieldForLabel>event_type</fieldForLabel>
      <fieldForValue>event_type</fieldForValue>
      <search>
        <query>index=overops metric=overops_events  service=$environment$ event_type!=caught_exception 
| dedup event_type 
| sort +event_type</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <choice value="*">All Events</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="radio" token="resurfaced" searchWhenChanged="false">
      <label>Resurfaced?</label>
      <choice value="*resurfaced*">Yes</choice>
      <choice value="*">No</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <table>
        <search>
          <query>index=overops metric=overops_jvms earliest=$time$ service=$environment$ | stats distinct_count(application) as JVMs distinct_count(server) as Servers | strcat "OverOps is connected to " JVMs " JVMs on " Servers " servers" string |rename string as "OverOps is Active" | table "OverOps is Active"</query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <sampleRatio>1</sampleRatio>
          <refresh>30s</refresh>
          <refreshType>delay</refreshType>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">none</option>
        <format type="number" field="JVMs">
          <option name="precision">0</option>
          <option name="unit">JVMs Monitored on</option>
        </format>
        <format type="number" field="Collectors">
          <option name="precision">0</option>
          <option name="unit">Collectors</option>
        </format>
      </table>
    </panel>
  </row>
